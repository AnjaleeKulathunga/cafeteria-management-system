# MongoDB Connection String
MONGODB_URL=mongodb+srv://username:password@cluster.mongodb.net/database_name?retryWrites=true&w=majority

# JWT Secret Key (use a strong random string in production)
JWT_SECRET=your_jwt_secret_key_here

# Stripe API Keys (get these from https://dashboard.stripe.com/apikeys)
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_PUBLIC_KEY=pk_test_your_stripe_public_key_here

# OpenAI API Key (if using OpenAI features)
OPENAI_API_KEY=your_openai_api_key_here
